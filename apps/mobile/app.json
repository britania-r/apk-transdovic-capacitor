      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB3f7SMf8Y4lHnJGvwMB9pcOm5HU__-blo"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "080332c6-6d7d-49e4-b7cf-4cbba15d7445"
